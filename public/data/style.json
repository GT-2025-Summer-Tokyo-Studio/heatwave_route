      "minzoom": 0,
      "maxzoom": 17.4,
      "layout": {
        "visibility": "none"
      }
    },
    {
      "id": "amap-image-label",
      "type": "raster",
      "source": "amap-image-label",
      "minzoom": 0,
      "maxzoom": 17.4,
      "layout": {
        "visibility": "none"
      }
    },
    {
      "id": "amap-vector",
      "type": "raster",
      "source": "amap-vector",
      "minzoom": 0,
      "maxzoom": 17.4,
      "layout": {
        "visibility": "visible"
      }
    },
    {
      "id": "background",
      "type": "background",
      "layout": {
        "visibility": "none"
      }
    }
  ]
